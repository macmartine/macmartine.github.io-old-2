---
pagination:
  data: collections
  size: 1
  alias: category
permalink: /categories/{{ category }}/
tags:
layout: layouts/base.njk
---
<h1>Category: {{ category }}</h1>

<ol>
{% set taglist = collections[ category ] | published %}
{% for post in taglist | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>
