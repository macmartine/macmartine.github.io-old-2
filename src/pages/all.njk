<h1>Essays</h1>

<h3>Published</h3>

<ol>
{% set taglist = collections.essay | published %}
{% for post in taglist | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>

